.flip-list {list-style: none;display: flex;justify-content: space-between;text-align: center;align-items: center;padding: 40px 0; padding-top: 0; color: #fff;}
.flip-list .flip-list-item {
    width: calc(100%/5);
    position: relative;
    border-right: 1px solid var(--form-border-color);
    height: 200px;
}
.flip-list .flip-list-item:last-child {border: none;}
.flip-list .flip-list-item:hover .front {opacity: 0;}
.flip-list .flip-list-item:hover .back {opacity: 1;}
.flip-list .flip-list-item .front {position: absolute;top: 50%;transform: translateY(-50%);width: 100%;font-size: 1.8rem;font-weight: bold;line-height: 1.5;transition: ease 1s all; text-transform: uppercase;}
.flip-list .flip-list-item .back {opacity: 0;padding: 10px;    position: absolute;
    top: 50%;
    transform: translateY(-50%);transition: ease 1s all;    line-height: 1.4;}

