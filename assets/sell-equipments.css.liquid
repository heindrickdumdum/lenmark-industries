/*     align-items: center; */
}


#sell-forms-modal .modal__content .sell-form .h2 {color: var(--primary-button-text-color);text-transform: none;font-size: 45px;}
#sell-forms-modal .modal__content .sell-form .h5 {font-size: 22px;color: var(--primary-button-text-color);text-transform: none;margin-bottom: 40px;}
#sell-forms-modal .modal__content .sell-form .modal__content-list {color: var(--header-accent-color);}

#sell-forms-modal .modal__content .bop-form .h2 {text-transform: none;font-size: 45px;}
#sell-forms-modal .modal__content .bop-form .h5 {font-size: 22px;text-transform: none;margin-bottom: 40px;}
#sell-forms-modal .modal__content .bop-form .modal__content-list {color: var(--primary-button-text-color);}

#sell-forms-modal .modal__content .sell-form .active-c-btn-submit {color: var(--secondary-background);background: var(--header-accent-color);}
#sell-forms-modal ul.modal__content-list {font-size: 23px;margin: 0;height: 185px;} 
#sell-forms-modal ul.modal__content-list .modal__content-list-item::before {display: none;}
#sell-forms-modal .modal__content .modal-image {width: 50%;}
#sell-forms-modal .modal__content .rte {width: 50%;}
#sell-forms-modal .modal__content .modal-image {display: none;}
#sell-forms-modal .modal__content .modal-image {position: relative;}
#sell-forms-modal .modal__content .modal-image .slick-arrow {position: absolute;top: 50%;color: #fff;font-size: 100px;padding: 0 30px;transform: translateY(-50%);z-index: 10;}
#sell-forms-modal .modal__content .modal-image .slick-arrow.slick-next {right: 0;}
#sell-forms-modal .modal__content .modal-image .slick-arrow[aria-disabled="true"] {color: #ddd;pointer-events: none;}
#sell-forms-modal .modal__content ul.slick-dots {position: absolute;bottom: 20px;width: 50%;text-align: center;}
#sell-forms-modal .modal__content ul.slick-dots li {display: inline;margin: 10px;}
#sell-forms-modal .modal__content ul.slick-dots li.slick-active button {background: #fff;}
#sell-forms-modal .modal__content ul.slick-dots li button {background: var(--primary-button-background);font-size: 0;width: 25px;height: 25px;border-radius: 20px;}

#sell-forms-modal .modal__close.link {position: absolute;right: 20px;top: 20px;color: #fff;}


#sell-forms-modal ul.modal__content-list .modal__content-list-item {position: relative;padding-left: 30px;}
#sell-forms-modal ul.modal__content-list .modal__content-list-item::before {
    display: block;
    content: "♦";
    background: none;
    position: absolute;
    top: 0;
    left: 0;
}

